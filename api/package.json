{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend App",
  "main": "index.js",
  "scripts": {
    "start": "node build/lib/index.js",
    "build": "tsc",
    "build:dev": "npm run generate && tsc",
    "pull_db": "npx prisma db pull",
    "generate": "npx prisma generate",
    "dev": "npm run generate && nodemon --exec ts-node index.ts"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "ajv": "^8.12.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mysql2": "^2.3.3",
    "sequelize": "^6.32.1",
    "ts-jest": "^29.1.1"
  },
  "devDependencies": {
    "jest-mock-extended": "^3.0.4",
    "jest-transform-stub": "^2.0.0",
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/node": "^16.18.38",
    "jest": "^29.5.0",
    "nock": "^13.0.4",
    "node": "^19.6.1",
    "nodemon": "^2.0.20",
    "prisma": "^4.16.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.9.5"
  },
  "jest": {
    "automock": false,
    "clearMocks": true,
    "coverageDirectory": "./testReports",
    "unmockedModulePathPatterns": [
      "node_modules"
    ],
    "preset": "ts-jest",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest",
      "\\.(svg|png|jpg)$": "jest-transform-stub"
    },
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "\\.(svg|png|jpg)$": "jest-transform-stub"
    },
    "rootDir": "./",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect",
      "./testUtils/setup.ts"
    ]
  },
  "nodemonConfig": {
    "signal": "SIGINT",
    "watch": [
      "./"
    ],
    "ext": "ts js tsx",
    "exec": "npm run dev",
    "env": {
      "NODE_ENV": "dev"
    }
  }
}
